{
  "files.autoSave": "onWindowChange",
  "editor.wordWrap": "on",
  "files.exclude": {
    "": true,
    "**.idea": true,
    "**.qodo": true,
    "**.yarn": true,
    "**/node_modules": true
  },
  "editor.bracketPairColorization.independentColorPoolPerBracketType": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#6a0b90",
    "titleBar.activeBackground": "#a912e5",
    "titleBar.activeForeground": "#F0FCFA"
  }
}
